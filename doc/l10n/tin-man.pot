#: doc/tin.5:2654
msgid ""
"B<\\%elm>(1), B<\\%inews>(1), B<\\%ispell>(1), B<\\%lp>(1), B<\\%lpr>(1), "
"B<\\%metamail>(1), B<\\%perl>(1), B<\\%pgp>(1), B<\\%rn>(1), B<\\%shar>(1), "
"B<\\%slrnface>(1), B<\\%tin>(1), B<\\%xterm>(1x), B<\\%heapsort>(3), "
