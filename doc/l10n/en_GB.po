#: ../tin.1:479
#, no-wrap
msgid "B<ScrollUp 'E<lt>'>"
msgstr "B<ScrollUp 'E<lt>'>"

# type: Plain text
#: ../tin.1:482
msgid "Scroll screen up by one line."
msgstr "Scroll screen up by one line."

# type: TP
#: ../tin.1:482
#, no-wrap
msgid "B<ScrollDown 'E<gt>'>"
msgstr "B<ScrollDown 'E<gt>'>"

# type: Plain text
